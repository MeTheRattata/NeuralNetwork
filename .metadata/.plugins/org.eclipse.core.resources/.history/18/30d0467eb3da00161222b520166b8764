import java.util.Scanner;
import java.io.BufferedWriter;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.OutputStreamWriter;
import java.io.Writer;

public class GermanWordParser 
{
	public static void main(String[] args)
	{
		File germanList = new File("src/GermanWordList1");
		File englishList = new File("src/EnglishWordList1");
		
		//int num5 = 0, num6 = 0, num7 = 0, num8 = 0, num9 = 0;

	    try {
	        Scanner in = new Scanner(germanList);
	        try (Writer writer = new BufferedWriter(new OutputStreamWriter(
	                new FileOutputStream("GermanWords.txt"), "utf-8"))) {
	        	
	        while (in.hasNext()) 
	        {
	            String str = in.next();
	            writer.write(str + '\n');
	            /*switch (str.length())
	            {
	            case 5:
	            	num5++;
	            case 6:
	            	num6++;
	            case 7:
	            	num7++;
	            case 8:
	            	num8++;
	            case 9:
	            	num9++;
	            }*/
	            
	            System.out.println(str);
	        }
	        
	        in = new Scanner(englishList);
	        
	  }
	        in.close();
	    } 
	    catch (FileNotFoundException e) {
	        e.printStackTrace();
	    }
	    
	    System.out.println("5: " + num5);
	    System.out.println("6: " + num6);
	    System.out.println("7: " + num7);
	    System.out.println("8: " + num8);
	    System.out.println("9: " + num9);
	}
}