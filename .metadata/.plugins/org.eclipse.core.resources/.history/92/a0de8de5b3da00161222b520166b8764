import java.util.Scanner;
import java.io.BufferedWriter;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.OutputStreamWriter;
import java.io.UnsupportedEncodingException;
import java.io.Writer;

public class GermanWordParser 
{
	public static void main(String[] args) throws UnsupportedEncodingException, IOException
	{
		File germanList = new File("src/GermanWordList1");
		File englishList = new File("src/EnglishWordList1");
		
		//int num5 = 0, num6 = 0, num7 = 0, num8 = 0, num9 = 0;

	    try {
	        Scanner in = new Scanner(germanList);
	        try (Writer writer = new BufferedWriter(new OutputStreamWriter(
	                new FileOutputStream("GermanWords.txt"), "utf-8"))) {
	        	
	        while (in.hasNext()) 
	        {
	            String str = in.next();
	            writer.write(str + '\n');
	            
	            System.out.println(str);
	        }
	        
	        in = new Scanner(englishList);
	        
	  }
	        in.close();
	    } 
	    catch (FileNotFoundException e) {
	        e.printStackTrace();
	    }
	}
}