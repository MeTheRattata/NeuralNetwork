import java.util.Scanner;
import java.io.File;
import java.io.FileNotFoundException;

public class GermanWordParser 
{
	public static void main(String[] args)
	{
		File germanList = new File("src/GermanWordList1");
		int num5 = 0, num6 = 0, num7 = 0, num8 = 0, num9 = 0;

	    try {
	        Scanner in = new Scanner(germanList);

	        while (in.hasNextLine()) 
	        {
	            String str = in.next();
	            switch (str.length())
	            {
	            case 5:
	            	num5++;
	            case 6:
	            	num6++;
	            case 7:
	            	num7++;
	            case 8:
	            	num8++;
	            case 9:
	            	num9++;
	            }
	            
	            System.out.println(str);
	        }
	        in.close();
	    } 
	    catch (FileNotFoundException e) {
	        e.printStackTrace();
	    }
	    
	    System.out.println("5: " + num5);
	    System.out.println("6: " + num6);
	    System.out.println("7: " + num7);
	    System.out.println("8: " + num8);
	    System.out.println("9: " + num9);
	}
}